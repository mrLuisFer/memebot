{
  "printWidth": 100,
  "tabWidth": 2,
  "semi": false,
  "trailingComma": "es5",
  "arrowParens": "always",
  "jsxSingleQuote": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "quoteProps": "as-needed",
  "useTabs": false,
  "htmlWhitespaceSensitivity": "css",
  "proseWrap": "preserve",
  "overrides": [
    {
      "files": "*.{js,jsx,tsx,ts,scss,json,html}",
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
